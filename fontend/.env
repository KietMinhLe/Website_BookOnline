
# App Configuration
REACT_APP_NAME=BookStore
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Ung dung ban sach truc tuyen

# API Configuration
REACT_APP_API_URL=http://localhost:3000/api
REACT_APP_API_TIMEOUT=10000

# Authentication
REACT_APP_JWT_SECRET=your_jwt_secret_key_here
REACT_APP_TOKEN_EXPIRY=24h

# Admin Configuration
REACT_APP_ADMIN_EMAIL=admin@bookstore.com
REACT_APP_ADMIN_PASSWORD=admin123

# Database Configuration
REACT_APP_DB_HOST=localhost
REACT_APP_DB_PORT=5432
REACT_APP_DB_NAME=bookstore
REACT_APP_DB_USER=postgres
REACT_APP_DB_PASSWORD=your_db_password

# Payment Gateway
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key
REACT_APP_PAYPAL_CLIENT_ID=your_paypal_client_id

# Email Service
REACT_APP_EMAIL_SERVICE=gmail
REACT_APP_EMAIL_USER=your_email@gmail.com
REACT_APP_EMAIL_PASSWORD=your_app_password

# File Upload
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif

# Social Login
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id
REACT_APP_FACEBOOK_APP_ID=your_facebook_app_id

# Analytics
REACT_APP_GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
REACT_APP_FACEBOOK_PIXEL_ID=your_facebook_pixel_id

# Development/Production
NODE_ENV=development
GENERATE_SOURCEMAP=false

# Build Configuration
REACT_APP_BUILD_DATE=2024-10-18
REACT_APP_BUILD_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PAYMENT=false
REACT_APP_ENABLE_SOCIAL_LOGIN=false
REACT_APP_ENABLE_EMAIL_NOTIFICATIONS=false
